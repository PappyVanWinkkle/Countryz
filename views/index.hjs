  <script src="countryz/js/jquery-1.11.3.min.js"></script>
  <script src="countryz/js/popper.min.js"></script>
  <script src="countryz/js/bootstrap.min.js"></script>
  <script src="countryz/js/datepicker.min.js"></script>
  <script src="countryz/js/jquery.singlePageNav.min.js"></script>
